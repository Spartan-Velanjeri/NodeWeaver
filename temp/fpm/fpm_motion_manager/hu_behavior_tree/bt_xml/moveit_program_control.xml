<?xml version="1.0"?>
<root main_tree_to_execute="BehaviorTree">
    <!-- ////////// -->
    <BehaviorTree ID="BehaviorTree">
        <Sequence>
            <Action ID="moveitcontrol"/>
            <Action ID="moveit" goal="0.178;0.413;0.518;0.0;0.0;0.00" name="Move to Home Position"/>
            <!-- <Action ID="moveit" goal="0.38;-0.315;0.75;0.0;0.0;0.60" name="move to Target Position"/> -->
            <Action ID="loadur" file_name="demo.urp"/>
            <Inverter>
                <KeepRunningUntilFailure>
                    <Sequence>
                        <Action ID="iostatecondition"/>
                    </Sequence>
                </KeepRunningUntilFailure>
            </Inverter>
            <Action ID="moveitcontrol"/>
        </Sequence>
    </BehaviorTree>
    <!-- ////////// -->
    <TreeNodesModel>
        <Action ID="iostatecondition"/>
        <Action ID="loadur">
            <input_port name="file_name" type="std::string"/>
        </Action>
        <Action ID="moveit">
            <input_port name="goal" type="Pose2D"/>
        </Action>
        <Action ID="moveitcontrol"/>
    </TreeNodesModel>
    <!-- ////////// -->
</root>
